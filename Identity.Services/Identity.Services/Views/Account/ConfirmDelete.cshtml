@model Identity.Services.Models.ApplicationUser
@{
    Layout = "_Layout";
}
<div class="container">
  <h1 class="text-danger">Are you sure you want to delete this user @Model.LastName @Model.FirstName</h1>
  <a asp-controller="Account" asp-action="Delete" asp-route-id="@Model.Id" 
     class="btn btn-default btn-danger">Delete</a>  
</div>
